@model Project.Service.VehicleModel
@{
    ViewBag.Title = "Details";
}

<h2>Model detailes</h2>

<table class="table">

    <tr>
        <th>@Html.DisplayNameFor(Model => Model.VehicleModelId)</th>
        <th>@Html.DisplayNameFor(Model => Model.Name)</th>
        <th>@Html.DisplayNameFor(Model => Model.Abrv)</th>
        <th>Maker Name</th>
    </tr>

    <tr>
    <tr>
        <td>@Model.VehicleModelId.ToString()</td>
